        SUBTRACT 1 FROM S
        IF S 255
            ENDGAME
            EXIT
        ENDIF

; Time bar

        LET T 0
        LET I S
        DIVIDE I BY 8
        AT 21,14
        IF I > 0
            REPEAT I
                PUTBLOCK 3
            ENDREPEAT
        ENDIF

        LET J S
        MULTIPLY I BY 8
        SUBTRACT I FROM J
        DIVIDE J BY 2
        IF J > 0
            ADD 3 TO J
            PUTBLOCK J
        ENDIF
        PUTBLOCK 48

    ENDIF
    ADD 1 TO T
    IF L > 0
        SUBTRACT 1 FROM L
    ENDIF
    IF B = 1
        SPAWN 2 12
        SPAWNED
        LET X Q
        IF H = 1
            ADD 16 TO X
            LET FRAME 0
        ELSE
            SUBTRACT 16 FROM X
            LET FRAME 4
        ENDIF
        LET Y P
        ENDSPRITE
        LET B 0
    ENDIF

; Lives bar

    IF Z 0
      AT 20,14
      LET C 0
      REPEAT 5
        IF LIVES > C
            PUTBLOCK 12
        ELSE
           PUTBLOCK 48
        ENDIF
        ADD 1 TO C
      ENDREPEAT
      LET O 0
      LET Z 1
    ENDIF

  ENDIF


