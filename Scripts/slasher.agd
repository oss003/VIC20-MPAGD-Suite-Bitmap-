EVENT SPRITETYPE3

IF D = 0
    ANIMATE
ENDIF
IF CANGOLEFT
    SPRITELEFT
ELSE
    SUBTRACT SETTINGB FROM R
    SUBTRACT 1 FROM O
    REMOVE
    BEEP 10
ENDIF
IF COLLISION 2
    SUBTRACT SETTINGB FROM R
    SUBTRACT 1 FROM O
    BEEP 60
    IF IMAGE 2
        SCORE 3
    ENDIF
    IF IMAGE 4
        SCORE 4
    ENDIF
    IF IMAGE 6
        SCORE 5
    ENDIF
    IF IMAGE 8
        SCORE 2
    ENDIF
    IF IMAGE 10
        SCORE 1
    ENDIF
    INK 7
    LET LINE 21
    LET COLUMN 7
    SHOWSCORE
    REMOVE
    LET N 0
    SPAWN 4 13
ENDIF
IF COLLISION 0
    SUBTRACT 64 FROM R
    SUBTRACT 1 FROM O
    SUBTRACT 1 FROM LIVES
    KILL
ENDIF


