;---------------------------------------------------------------
; Player
;---------------------------------------------------------------

EVENT PLAYER

GETBLOCKS

IF H = 0			; Check if fire falling

  TABLEFALL

  IF CUSTOM			; Balloon trigger
    IF C 0			; Set balloon flag
      LET C 1
      SUBTRACT 17 FROM Y
      SPAWN 8,10		; Plot balloon
      ADD 17 TO Y
    ENDIF
  ENDIF

  IF C 1			; If balloon on screen
    LET I 0			; Check if on platform
    IF Y 80			; Plaform 2
      LET I 1
    ENDIF
    IF Y 120			; Plaform 3
      LET I 1
    ENDIF
    IF Y 160			; Plaform 4
      LET I 1
    ENDIF

    IF I 0			; Not on platform
      ADD 1 TO Y		; Fall
    ELSE
      LET T 9			; On platform, reset speed
      LET C 0			; Reset balloon flag
    ENDIF

    EXIT			; Exit sub
  ENDIF

  LET S = T			; Set speed
  DIVIDE S BY 10

  IF S > 0			; Move player with speed delay
    REPEAT S

      IF X < 240		; Check leaving screen right
        SPRITERIGHT
      ELSE
        LET X 0
	IF Y < 160		; End of screen not reached
	  ADD 40 TO Y
 	  SCORE B
          AT 1,8
          INK 1
          SHOWSCORE 5
	ELSE 
 	  SCORE B		; End of screen reached
          AT 1,8
          INK 1
          SHOWSCORE 5
	  NEXTLEVEL
	  ADD 10 TO B
	ENDIF
      ENDIF
    ENDREPEAT
  ENDIF

  IF KEY RIGHT			; Accelerate
   IF T < 40
    ADD 1 TO T
   ENDIF
  ENDIF

  IF KEY LEFT			; Break, once per screen
   IF Z = 0
    STOPFALL
    LET T 0
    LET Z 1
   ENDIF
  ENDIF

  IF KEY FIRE			; Jump
   IF H = 0
     TABLEJUMP
   ENDIF
  ENDIF

ENDIF

IF DEADLY			; Check if hit static object
  IF H = 0
    LET FRAME 3			; If so, change player in fire
    SPAWN 2 3			; Plot player dead
    LET H 1
    LET S 0
    LET C 0
  ENDIF
ENDIF

;---------------------------------------------------------------
; Horizontal moving enemy, ambulance, sailing boat, snowball
;---------------------------------------------------------------

EVENT SPRITETYPE1

  IF COLLISION 0		; If hit player
    OTHER
      IF H = 0
        LET FRAME 3		; Change player in fire
        SPAWN 2 3		; Plot dead player
        LET H 1
        LET C 0
      ENDIF
    ENDSPRITE
  ENDIF

  SPRITERIGHT			; Move horizontal enemy
  SPRITERIGHT
  ANIMATE

;---------------------------------------------------------------
; Dead player
;---------------------------------------------------------------

EVENT SPRITETYPE2

  TABLEFALL

  IF AIRBORNE > 0		; If in air
    SPRITERIGHT			; Move right
    SPRITERIGHT
  ELSE
    ADD 1 TO SETTINGA
    IF SETTINGA < 20		; Animate 20 times
      ANIMATE
    ELSE
      KILL
    ENDIF
  ENDIF

;---------------------------------------------------------------
; Spider
;---------------------------------------------------------------

EVENT SPRITETYPE3

  ADD 1 TO SETTINGA

  IF SETTINGA 4			; Check move up/down
    IF D 0
      SPRITEDOWN		; Move down
      IF Y > 150		
        LET D 1			; If at bottom, change direction to up
      ENDIF
    ELSE
      SPRITEUP			; Move up
      IF Y < 142
        LET D 0			; If at top, change direction down
      ENDIF
    ENDIF
    ANIMATE
    LET SETTINGA 0
  ENDIF

;---------------------------------------------------------------
; Spider string
;---------------------------------------------------------------

EVENT SPRITETYPE4

  ADD 1 TO SETTINGA

  IF SETTINGA 4			; Check move up/down
    IF D 0
      ANIMATE			; If down enlarge string
    ELSE
      ANIMBACK			; If up shorten string
    ENDIF
    LET SETTINGA 0
  ENDIF

;---------------------------------------------------------------
; Policeman
;---------------------------------------------------------------

EVENT SPRITETYPE5

  IF SETTINGA > 0		; Delaytime start jump
    SUBTRACT 1 FROM SETTINGA
  ELSE
    READ I			; Get displacement
    ADD I TO Y			; Move policeman
    IF COLLISION 0		; If hi player
      OTHER
        IF H = 0
          LET FRAME 3		; If so, change player in fire
          SPAWN 2 3		; Plot player dead
          LET H 1
          LET C 0
        ENDIF
      ENDSPRITE
    ENDIF
  ENDIF

  DATA $fb,0,$fd,0,$fd,0,$fe,0,$fe,0,$ff,0,$ff,0,$ff,0,0,0,0,0,1,0,1,0,1,0,2,0,2,0,3,0,3,0,5,0

;---------------------------------------------------------------
; Helicopter
;---------------------------------------------------------------

EVENT SPRITETYPE6

  SPRITELEFT			; Move helicopter
  SPRITELEFT

  IF X = 0			; If leaving screen left, platform up
    IF Y > 24
      SUBTRACT 40 FROM Y
    ELSE
      LET Y 144
    ENDIF
    LET X 240
  ENDIF

  IF COLLISION 0		; If collision player
    OTHER
      IF H = 0
        LET FRAME 3		; Change player in fire
        SPAWN 2 3		; Plot dead player
        LET H 1
        LET C 0
      ENDIF
    ENDSPRITE
  ENDIF

  ANIMATE			; Animate rotor

  IF Y <> 64			; If not on platform2

    IF U 0			; Check if bonusbar falling
      READ V			; Get dropping point
       LET U 1			; No, set Bonusbar flag
    ENDIF

    IF W < 8			; If less than 8 bonusbars
      IF X = V			; If helicopter at dropping point
        LET I X
        LET J Y
        DIVIDE I BY 8
        MULTIPLY I BY 8
        ADD 31 TO J
        GETBLOCK I,J
        IF BLOCK EMPTYBLOCK	; Check for static enemy
          ADD 8 TO J
          GETBLOCK I,J
          IF BLOCK PLATFORMBLOCK; Check for platform
            SPAWN 7 9		; If ok, drop bonusbar
            SPAWNED
              LET X I
              LET SETTINGA 0
            ENDSPRITE
            ADD 1 TO W		; Increment bonusbar counter
;          ELSE
;            LET U 0
          ENDIF
        ENDIF
      ENDIF
    ENDIF

  ENDIF

  DATA 200,48,188,24,76,40,108	; Bonusbar dropping points

;---------------------------------------------------------------
; Falling bonusbar
;---------------------------------------------------------------

EVENT SPRITETYPE7

  IF SETTINGA 8			; If bar at platform
    TABLEFALL
    IF AIRBORNE = 0		; If on platform
      REMOVE			; Remove bar
      LET LINE = Y
      LET COLUMN = X
      DIVIDE LINE BY 8
      DIVIDE COLUMN BY 8
      ADD 1 TO LINE
      LET O LINE
      LET P COLUMN
      LET SETTINGA 0
      LET U 0			; Reset bonusbar falling flag
    ENDIF
    SETTINGA 0
  ELSE
    ADD 1 TO SETTINGA		; If not on platform, fall
  ENDIF

;---------------------------------------------------------------
; Balloon
;---------------------------------------------------------------

EVENT SPRITETYPE8

  IF C 0			; Player landed on platform
    REMOVE			; Erase balloon
  ENDIF

  IF CANGODOWN			; Check if falling
    ADD 1 TO Y			; Move down
  ENDIF

;---------------------------------------------------------------
; Initialise sprite
;---------------------------------------------------------------

EVENT INITSPRITE

  IF TYPE 0			; If player

    LET I SCREEN		; Select theme
    LET J SCREEN
    DIVIDE J BY 3
    MULTIPLY J BY 3
    SUBTRACT J FROM I

    IF I 0
      LET FRAME 0		; Player on motorbike
    ENDIF

    IF I 1
      LET FRAME 1		; Player on jetski
    ENDIF

    IF I 2
      LET FRAME 2		; Player on skies
    ENDIF
  ENDIF

  IF TYPE 2			; If horizontal moving enemy
     LET DIRECTION 3
     LET SETTINGA 0
     TABLEJUMP
  ENDIF

  IF TYPE 3			; If Spider
     LET SETTINGA 0
     LET FRAME 0
  ENDIF

  IF TYPE 4			; If spider string
     LET SETTINGA 0
     LET FRAME 0
  ENDIF

  IF TYPE 5			; Set delaytime policeman
    GETRANDOM 12
    LET SETTINGA RND
  ENDIF

;---------------------------------------------------------------
; Mainloop1
;---------------------------------------------------------------

EVENT MAINLOOP1

  IF O > 0			; Check if bonusbar fallen
    LET LINE O			; Set coordinates
    LET COLUMN P
    PUTBLOCK 9			; Plot collectable block
    LET O 0			; Reset flag
  ENDIF

  IF KEY OPTION1		; Debug, next level
    NEXTLEVEL
  ENDIF

;---------------------------------------------------------------
; Mainloop 2
;---------------------------------------------------------------

EVENT MAINLOOP2

;---------------------------------------------------------------
; Intro menu
;---------------------------------------------------------------

EVENT INTROMENU

  PAPER 0			; Clear screen
  CLS

  INK 3				; Print topline
  AT 1,3
  PRINTMODE 1
  PRINT "HIGHEST WAGES PAID $"
  SHOWHIGH 5
  PRINT "0"
  PRINTMODE 0

  INK 2				; Print second line
  AT 4,8
  PRINT "HOW GOOD ARE YOU"

  INK 1				; Print options
  AT 7,10
  INK 3
  PRINT "A)"
  INK 1
  PRINT " NOVICE"
  AT 9,10
  INK 3
  PRINT "B)"
  INK 1
  PRINT " EXPERIENCED"
  AT 11,10
  INK 3
  PRINT "C)"
  INK 1
  PRINT " EXPERT"
  INK 3
  AT 13,10
  PRINT "D)"
  INK 1
  PRINT " OSCAR NOMINEE"
  INK 3
  AT 15,10
  PRINT "E)"
  INK 1
  PRINT " OSCAR WINNER"
  INK 3
  AT 17,10
  PRINT "F)"
  INK 1
  PRINT " ACE"

  INK 3				; Print keys
  AT 20,4
  INK 2
  PRINT "UP="
  INK 1
  PRINT "Q"
  INK 2
  PRINT " DOWN="
  INK 1
  PRINT "A"
  INK 2
  PRINT " START="
  INK 1
  PRINT "SPACE"

  AT 22,0			; Scrollbar
  INK 1
  TICKER 32,"DAREDEVIL DENNIS ...... WRITTEN BY SIMON PICK .... VISIONS PRESENTS ......... "

  INK 3				; Make selection until SPACE
  LET I 0
  LET J 7
  LET K 0
  AT J,8
  PRINT ">"

  WHILE I 0
    WAITKEY

    IF KEY DOWN			; Move selection down
      IF J < 17
        AT J,8
        PRINT " "
        ADD 2 TO J
        AT J,8
        PRINT ">"
        DELAY 10
      ENDIF
    ENDIF 

    IF KEY UP			; Move selection up
     IF J > 7
       AT J,8
       PRINT " "
       SUBTRACT 2 FROM J
       AT J,8
       PRINT ">"
       DELAY 10
     ENDIF
    ENDIF  
 
    IF KEY FIRE 		; Select option
      WAITKEY  
      DELAY 5 0
      LET I 1
    ENDIF

  ENDWHILE

  TICKER 0			; Remove scrolline

  LET A 0			; Reset vars
  LET D 0
  LET Z 0
  LET LIVES 3

;---------------------------------------------------------------
; Game initialisation
;---------------------------------------------------------------

EVENT GAMEINIT

  IF J 7			; If selection = 'A'
    LET SCREEN 0
  ENDIF

  IF J 9			; If selection = 'B'
    LET SCREEN 6
  ENDIF

  IF J 11			; If selection = 'C'
    LET SCREEN 12
  ENDIF

  IF J 13			; If selection = 'D'
    LET SCREEN 18
  ENDIF

  IF J 15			; If selection = 'E'
    LET SCREEN 24
  ENDIF

  IF J 17			; If selection = 'F'
    LET SCREEN 30
  ENDIF

;---------------------------------------------------------------
; Restart screen
;---------------------------------------------------------------

EVENT RESTARTSCREEN

  PAPER 2			; Clear screen
  CLS

  INK 1				; Display TAKE x
  AT 12,13
  PRINT "TAKE "
  LET I 3
  SUBTRACT LIVES FROM I
  ADD 4 TO I
  PUTBLOCK I
  AT 11,18
  PUTBLOCK 7
  DELAY 100
  AT 11,18
  PUTBLOCK 8
  DELAY 150

  PAPER 2			; Clear screen
  CLS

  AT 1,2			; Print banner Wages
  INK 0
  PRINT "WAGES "
  INK 1
  SHOWSCORE 5
  PRINT "0"

  LET B SCREEN			; Set bonus per platform
  ADD 1 TO B

  LET D 0			; Reset vars
  LET H 0
  LET T 0
  LET U 0
  LET W 0
  LET Z 0

;---------------------------------------------------------------
; Fell too far
;---------------------------------------------------------------

EVENT FELLTOOFAR

;---------------------------------------------------------------
; Kill player
;---------------------------------------------------------------

EVENT KILLPLAYER

  SUBTRACT 1 FROM LIVES
  IF LIVES > 0
    DELAY 100			; If lives left, restart level
    RESTART
  ENDIF

;---------------------------------------------------------------
; Game lost
;---------------------------------------------------------------

EVENT LOSTGAME

  PAPER 0			; No lives left, end game
  LET I 0
  PRINTMODE 1			; Big text
  REPEAT 15
    INK I			; Change colour
    AT 13,9
    PRINT "YOU ARE FIRED!"
    DELAY 10
    ADD 1 TO I
    IF I = 4
      I = 0
    ENDIF
  ENDREPEAT
  PRINTMODE 0			; Normal text
  DELAY 60

;---------------------------------------------------------------
; Game completed
;---------------------------------------------------------------

EVENT COMPLETEDGAME

  PAPER 2
  CLS				; Clear blue screen

  INK 3
  AT 11,6
  PRINT "All levels completed."

  INK 3
  AT 13,10
  PRINT "SCORE: "
  SHOWSCORE 5
  PRINT "0"

  INK 0
  AT 18,5
  PRINT "Press any key to restart"

  PRINTMODE 1			; Big text
  LET I 0
  REPEAT 14
    INK I			; Change colour
    AT 8,8
    PRINT "CONGRATULATIONS!"
    DELAY 10
    ADD 1 TO I
    IF I = 4
      I = 0
    ENDIF
  ENDREPEAT
  PRINTMODE 0			; Normal text

  WAITKEY

;---------------------------------------------------------------
; New highscore
;---------------------------------------------------------------

EVENT NEWHIGHSCORE

  PAPER 2
  CLS				; Clear blue screen

  INK 3
  AT 11,10
  PRINT "SCORE: "
  SHOWSCORE 5
  PRINT "0"

  PRINTMODE 1			; Big text
  LET I 0
  REPEAT 14
    INK I			; Change colour
    AT 8,8
    PRINT "NEW HIGHSCORE!!!"
    DELAY 10
    ADD 1 TO I
    IF I = 4
      I = 0
    ENDIF
  ENDREPEAT
  PRINTMODE 0			; Normal text

  DELAY 80

