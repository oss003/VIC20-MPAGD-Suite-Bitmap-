; Collectable block
;---------------------------------------------------------------

EVENT COLLECTBLOCK

  SCORE 1			; Add 10 to score
  AT 1,8			; Showscore
  INK 1
  SHOWSCORE 5
  PRINT "0"

  SUBTRACT 1 FROM W		; Decrement bonusbar counter
